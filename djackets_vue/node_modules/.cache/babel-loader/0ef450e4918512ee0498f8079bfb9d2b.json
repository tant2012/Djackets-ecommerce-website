Route.value.params.category_slug;\n  store.setIsLoading(true);\n\n  await http\n    .get(`/v1/products/${categorySlug}/`)\n    .then((response) => {\n      category.value = response.data;\n\n      document.title = category.value.name + \" | TGHomie\";\n    })\n    .catch((error) => {\n      console.log(error);\n\n      toast({\n        message: \"Something went wrong. Please try again.\",\n        type: \"is-danger\",\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: \"bottom-right\",\n      });\n    });\n  store.setIsLoading(false);\n};\n\nonMounted(() => {\n  getCategory();\n});\n\nwatch(\n  () => router.currentRoute.value,\n  (to) => {\n    if (to.name === \"Category\") {\n      getCategory();\n    }\n  }\n);\n</script>\n\n<style></style>\n"]}]}