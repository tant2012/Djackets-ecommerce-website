 => {\n  store.setIsLoading(true);\n\n  await http\n    .get(\"/v1/orders/\")\n    .then((response) => {\n      orders.value = response.data;\n    })\n    .catch((error) => {\n      console.log(error);\n    });\n  store.setIsLoading(false);\n};\n\nonMounted(() => {\n  document.title = \"My account | TGHomie\";\n\n  getMyOrders();\n});\n</script>\n"]}]}